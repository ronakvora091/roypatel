<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ServletRonak</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    
    
  </welcome-file-list>
  
  <!-- First Servvlet -->
  
  <servlet>
  <description>RegisterController</description>
  
  <servlet-name>RegisterController</servlet-name>
  <servlet-class>com.practice.app.controller.RegisterController</servlet-class>
  
  </servlet>
  
  <!-- param value defined -->
  
  <context-param>
  <param-name>dname</param-name>
  <param-value>Ronak vora</param-value>
  </context-param>
  
  
  <servlet-mapping>
	  <servlet-name>RegisterController</servlet-name>
	  <url-pattern>/RegisterController</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  <description>LoginController</description>
  
  <servlet-name>LoginController</servlet-name>
  <servlet-class>com.practice.app.controller.LoginController</servlet-class>
  
  </servlet>
  
  <servlet-mapping>
	  <servlet-name>LoginController</servlet-name>
	  <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  
</web-app>